{
  "name": "nicehash-calculator-web",
  "version": "1.0.0",
  "description": "Web interface for github.com/GarboMuffin/nicehash-calculator",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "build": "npm run build-sass && npm run build-babel",
    "build-sass": "node-sass --output-style expanded -o public/ src/public/",
    "build-sass-min": "node-sass --output-style compressed -o public/ src/public/",
    "watch-sass": "node-sass --output-style expanded -o public/ src/public/ -w",
    "build-babel": "babel src/public -d public",
    "watch-babel": "babel src/public -d public -w"
  },
  "author": "GarboMuffin",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.1",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "pug": "^2.0.0-rc.4",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "private": true
}
